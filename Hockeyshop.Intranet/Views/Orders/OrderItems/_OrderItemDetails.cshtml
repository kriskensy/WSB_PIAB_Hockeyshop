@model Hockeyshop.Data.Data.Orders.OrderItem

<dt class="col-sm-4">Order Date</dt>
<dd class="col-sm-8">@Model.Order?.OrderDate</dd>
<dt class="col-sm-4">Product</dt>
<dd class="col-sm-8">@Model.Product?.Name</dd>
<dt class="col-sm-4">Quantity</dt>
<dd class="col-sm-8">@Model.Quantity</dd>
<dt class="col-sm-4">Unit Price</dt>
<dd class="col-sm-8">@Model.UnitPrice.ToString("C")</dd>