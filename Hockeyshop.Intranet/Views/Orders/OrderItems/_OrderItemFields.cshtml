@model Hockeyshop.Data.Data.Orders.OrderItem

<div class="form-group">
    <label asp-for="IdOrder" class="control-label"></label>
    <select asp-for="IdOrder" class="form-control" asp-items="ViewBag.IdOrder"></select>
</div>
<div class="form-group">
    <label asp-for="IdProduct" class="control-label"></label>
    <select asp-for="IdProduct" class="form-control" asp-items="ViewBag.IdProduct"></select>
</div>
<div class="form-group">
    <label asp-for="Quantity" class="control-label"></label>
    <input asp-for="Quantity" class="form-control" />
    <span asp-validation-for="Quantity" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="UnitPrice" class="control-label"></label>
    <input asp-for="UnitPrice" class="form-control" />
    <span asp-validation-for="UnitPrice" class="text-danger"></span>
</div>