@model dynamic
@{
    string partialDetails = ViewData["PartialDetails"] as string;
    string title = ViewData["Title"] as string ?? "Details";
}

<div class="row">
    <div class="col-md-6">
        <div class="card p-3">
            <h3 class="mb-3">@title</h3>
            <dl class="row">
                @await Html.PartialAsync(partialDetails, Model)
            </dl>
            <div class="mt-3">
                <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
                <a asp-action="Index" class="btn btn-secondary ml-2">Back to list</a>
            </div>
        </div>
       
    </div>
</div>
