@model Hockeyshop.Data.Data.CMS.HockeyNews

<dt class="col-sm-4">Title</dt>
<dd class="col-sm-8">@Model.Title</dd>
<dt class="col-sm-4">Content</dt>
<dd class="col-sm-8">@Model.Content</dd>
<dt class="col-sm-4">Author</dt>
<dd class="col-sm-8">@Model.IdAuthor</dd>
<dt class="col-sm-4">Created At</dt>
<dd class="col-sm-8">@Model.CreatedAt.ToShortDateString()</dd>
<dt class="col-sm-4">Published</dt>
<dd class="col-sm-8">@Model.Published</dd>
<dt class="col-sm-4">Image Url</dt>
<dd class="col-sm-8">@Model.ImageUrl</dd>